 /* DCLGEN TABLE(IBMUSER.NABAVA)                                      */
 /*        LIBRARY(ANGI.INCLUDE(NABAVA))                              */
 /*        LANGUAGE(PLI)                                              */
 /*        STRUCTURE(NABAVA)                                          */
 /*        APOST                                                      */
